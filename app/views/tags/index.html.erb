
<h1>All Tags</h1>

<ul id="tags">
	<% @tags.each do |tag| %>
		<li>
			<%= link_to tag.name, tag, class: 'tag_title' %>
			<% if logged_in? %>
				<%= link_to "delete", tag, method: :delete, data: {confirm: "Really Delete this Tag?"} %>
			<% end %>
		</li>
	<% end %>
</ul>
<br/>
<%= link_to "<< Back to Articles List", articles_path %>





